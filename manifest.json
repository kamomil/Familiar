{
  "name": "Familiar",
  "version": "0.1",
  "description": "Helps with improving vocabulary by saving all words you ever ancountered",
  "icons": { "128": "icons/orange-f1.png",
              "128": "icons/orange-f1.ico",
              "128": "icons/orange-f2.ico"
            },
  "background": { "scripts": ["background.js", "localstorage.js", "jquery-3.3.1.js"] },
  "permissions": [
      "storage",
      "http://*/*",
      "https://*/*",
      "file:///home/*",
      "contextMenus",
      "tabs"
  ],

  "browser_action": {
      "default_name": "F",
      "default_icon": "icon.png" 
  },
  "manifest_version": 2
}
